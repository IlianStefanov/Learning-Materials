{
    "parser": "babel-eslint",
    "plugins": [
        "cypress"  // "chai-friendly"
    ],
    "rules": {
        "no-console": "warn",
        "no-unused-expressions": 0
        // "chai-friendly/no-unused-expressions": 2
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "js": true
        }
    }
    // "extends": ["airbnb-base"]
}